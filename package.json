{
  "name": "atomify-kit",
  "version": "1.3.8",
  "description": "React Atoms Components Based On Ant Design",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "modules.root": "./es",
  "jsnext:main": "./es/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "es",
    "lib",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:lib": "babel src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\"",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js -o dist/atomify-kit.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js -o dist/atomify-kit.min.js",
    "clean": "rimraf dist lib es",
    "build": "npm run clean && npm run build:lib && npm run build:es && npm run build:umd && npm run build:umd:min",
    "storybook": "start-storybook -p 9001 -c ./storybook/.storybook",
    "build-storybook": "build-storybook",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm lint"
    }
  },
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helabenkhalfallah/atomify.git"
  },
  "keywords": [
    "react",
    "antd"
  ],
  "author": "Ben Khalfallah HÃ©la",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/helabenkhalfallah/atomify/issues"
  },
  "homepage": "https://github.com/helabenkhalfallah/atomify#readme",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "=16.12.0",
    "react-dom": "=16.12.0",
    "prop-types": "=15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "=7.7.4",
    "@babel/core": "=7.6.0",
    "@babel/plugin-proposal-class-properties": "=7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "=7.5.5",
    "@babel/plugin-proposal-optional-chaining": "=7.2.0",
    "@babel/plugin-syntax-dynamic-import": "=7.2.0",
    "@babel/plugin-transform-react-jsx": "=7.7.0",
    "@babel/plugin-transform-runtime": "=7.6.2",
    "@babel/polyfill": "=7.7.0",
    "@babel/preset-env": "=7.6.0",
    "@babel/preset-react": "=7.0.0",
    "@storybook/addon-actions": "=5.2.6",
    "@storybook/addon-console": "=1.2.1",
    "@storybook/addon-info": "=5.2.6",
    "@storybook/addon-knobs": "=5.2.6",
    "@storybook/addon-links": "=5.2.6",
    "@storybook/addon-options": "=5.2.6",
    "@storybook/addons": "=5.2.6",
    "@storybook/react": "=5.2.6",
    "antd": "=4.0.0-alpha.11",
    "babel-eslint": "=10.0.3",
    "babel-loader": "=8.0.6",
    "babel-plugin-import": "=1.12.2",
    "cross-env": "^6.0.3",
    "css-loader": "=3.2.0",
    "eslint": "=6.6.0",
    "eslint-config-airbnb": "=18.0.1",
    "eslint-plugin-import": "=2.18.2",
    "eslint-plugin-jsx-a11y": "=6.2.3",
    "eslint-plugin-react": "=7.16.0",
    "eslint-plugin-react-hooks": "=1.7.0",
    "less": "=3.10.3",
    "less-loader": "=5.0.0",
    "mini-css-extract-plugin": "=0.8.0",
    "react": "=16.12.0",
    "react-dom": "=16.12.0",
    "storybook-readme": "=5.0.8",
    "style-loader": "=1.0.0",
    "terser-webpack-plugin": "=2.2.1",
    "webpack": "=4.41.2",
    "webpack-bundle-analyzer": "=3.6.0",
    "webpack-cli": "=3.3.10",
    "webpack-dev-server": "=3.9.0"
  }
}